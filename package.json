    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "config": "^3.3.12",
    "debug": "^4.3.6",
    "express": "^4.19.2",
    "hbs": "^4.2.0",
    "http-errors": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.22",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4"
  }
}
